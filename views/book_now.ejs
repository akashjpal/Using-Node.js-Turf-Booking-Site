<body>
    <div id="booking" class="section">
        <div class="section-center">
            <div class="container">
                <div class="row">
                    <div class="booking-form">
                        <div class="booking-bg"></div>
                        <form action="/book_now" method="post">
                            <div class="form-header">
                                <h2>Make your reservation</h2>
                            </div>
                            <h3 class="form-label">Turfname</h3>
                            <input type="text" name="turfname" value="<%=oneturf.name%>">
                            <h3 class="form-label">Location:</h3>
                            <input type="text" name="location" value="<%=oneturf.location%>">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <span class="form-label">Check In</span>
                                        <input class="form-control" type="date" name="check_in_date" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <span class="form-label">Check Out</span>
                                        <input class="form-control" type="date" name="check_out_date" required>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <span class="form-label">Check In Time</span>
                                        <select class="form-control" name="timeslot">
                                            <option>---Select your Timing slot---</option>
                                            <option>Morning</option>
                                            <option>Noon</option>
                                            <option>Evening</option>
                                            <option>Night</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <span class="form-label">Members</span>
                                        <select class="form-control" name="members">
                                            <option>---Select your group Members---</option>
                                            <option>solo</option>
                                            <option>group of 2-5</option>
                                            <option>group of 6-10</option>
                                            <option>more than 10</option>
                                        </select>
                                        <span class="select-arrow"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <span class="form-label">Email</span>
                                <input class="form-control" type="email" name="email" placeholder="Enter your email">
                            </div>
                            <div class="form-group">
                                <span class="form-label">Phone</span>
                                <input class="form-control" type="tel" name="phone"
                                    placeholder="Enter your phone number">
                            </div>


                            <button class="submit-btn">Book Now</button>

                    </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    </div>

</body>
